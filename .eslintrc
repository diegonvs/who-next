{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
  	"camelcase":"off",
  	"object-curly-spacing": "off"
	},
	"parserOptions": {
		"ecmaFeatures": {
			"allowImportExportEverywhere": true,
			"experimentalObjectRestSpread": true,
			"jsx": true
		},
		"ecmaVersion": 2017,
		"sourceType": "module"
	}
}
